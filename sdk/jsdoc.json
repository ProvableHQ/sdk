{
  "source": {
    "include": ["sdk/src/account.ts", "sdk/src/function-key-provider.ts", "sdk/src/network-client.ts", "sdk/src/offline-key-provider.ts", "sdk/src/program-manager.ts", "sdk/src/record-provider.ts"],
    "includePattern": ".+\\.ts?$"
  },
  "tags": {
    "allowUnknownTags": ["optional"]
  },
  "plugins": ["node_modules/better-docs/typescript"],
  "opts": {
    "encoding": "utf8",
    "readme": "./README.md",
    "destination": "sdk/docs/",
    "recurse": true,
    "verbose": true,
    "template": "./node_modules/clean-jsdoc-theme",
    "theme_opts": {
      "default_theme": "dark",
      "static_dir": ["sdk/docs/public"],
      "homepageTitle": "Provable SDK"
    }
  },
  "markdown": {
    "hardwrap": false,
    "idInHeadings": true
  }
}
